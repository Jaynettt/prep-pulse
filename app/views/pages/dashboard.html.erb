      <hr>
      <% @pulses.each do |pulse| %>
        <ul>
          <%= link_to pulse.job_role, pulse_path(pulse), data: {turbo_frame: "main_content"} %>
        <hr>
        </ul>
      <% end %>
       <%#= link_to new_pulse_path, class:"icon-add" do %>
<!--       <i class="fa-solid fa-square-plus"></i>-->
       <%# end %>


    </div>




    <div class="main-content">
      <%= turbo_frame_tag "main_content" do %>
        <h1 class="text-center">Content</h1>
      <% end %>
    </div>






</div>
